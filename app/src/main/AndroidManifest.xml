<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="management.project.spring.com.projectmanagement">

  <uses-permission android:name="android.permission.INTERNET"/>

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name=".AddResourcesActivity">
    </activity>

    <meta-data
        android:name="preloaded_fonts"
        android:resource="@array/preloaded_fonts"/>

    <activity android:name=".AssignmentActivity">
    </activity>
    <activity android:name=".AddProjectActivity">
    </activity>
    <activity android:name=".ProjectModifyActivity">
    </activity>
    <activity android:name=".EditResourcesActivity">
    </activity>
  </application>

</manifest>